<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alec.positive_eating">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginWindow">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainScreen" />
        <activity android:name=".OrderStatus" />
        <activity android:name=".Menu" />
        <activity android:name=".Seating" />
        <activity android:name=".Schedule" />
        <activity android:name=".EditStaff" />
        <activity android:name=".Payroll" />
        <activity android:name=".add_employee" />
        <activity android:name=".Clocking_in" />

        <service
            android:name=".ShaneConnectService"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name=".order_menu"
            android:label="@string/title_activity_order_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".tableMap" />
        <activity android:name=".viewTableMap"></activity>
    </application>

</manifest>